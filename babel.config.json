{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    ["i18next-extract", {
      "locales": ["en", "ru"],
      "defaultNS": "translation",
      "outputPath": "public/locales/{{locale}}/{{ns}}.json",
      "keyAsDefaultValue": true
    }]
  ]
}
